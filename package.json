    "start-local": "yarn link react-interactive && yarn start",
    "start": "webpack-dev-server --devtool eval-source-map --host 0.0.0.0 --public 10.0.1.4:8080 --history-api-fallback --open",
    "unlink-dev": "yarn unlink react-interactive && yarn add react-interactive",
    "build": "yarn unlink-dev && webpack -p"
